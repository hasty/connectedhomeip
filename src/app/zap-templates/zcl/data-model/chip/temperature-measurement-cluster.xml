<?xml version="1.0"?>
<configurator>
  <cluster>
    <attribute code="0x0000" define="TEMP_MEASURED_VALUE" isNullable="true" max="0x7fff" min="0x954d" optional="false" side="server" type="temperature" writable="false">MeasuredValue</attribute>
