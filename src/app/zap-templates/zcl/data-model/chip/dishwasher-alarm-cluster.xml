<?xml version="1.0"?>
<configurator>
  <bitmap name="AlarmMap" type="bitmap32">
    <cluster code="0x005D"/>
    <field mask="0x01" name="InflowError"/>
    <field mask="0x02" name="DrainError"/>
    <field mask="0x04" name="DoorError"/>
    <field mask="0x08" name="TempTooLow"/>
    <field mask="0x10" name="TempTooHigh"/>
    <field mask="0x20" name="WaterLevelError"/>
  </bitmap>
  <bitmap name="Feature" type="bitmap32">
    <cluster code="0x005D"/>
    <field mask="0x01" name="Reset"/>
  </bitmap>
  <cluster>
    <attribute code="0x0000" default="0" define="MASK" optional="false" side="server" type="AlarmMap" writable="false">Mask</attribute>
    <attribute code="0x0001" default="0" define="LATCH" optional="true" side="server" type="AlarmMap" writable="false">Latch</attribute>
    <attribute code="0x0002" default="0" define="STATE" optional="false" side="server" type="AlarmMap" writable="false">State</attribute>
    <attribute code="0x0003" default="0" define="SUPPORTED" optional="false" side="server" type="AlarmMap" writable="false">Supported</attribute>
    <client init="false" tick="false">true</client>
    <code>0x005D</code>
    <command code="0x00" name="Reset" optional="true" source="client">
      <arg name="Alarms" optional="false" type="AlarmMap"/>
      <description>Reset alarm</description>
    </command>
    <command code="0x01" name="ModifyEnabledAlarms" optional="true" source="client">
      <arg name="Mask" optional="false" type="AlarmMap"/>
      <description>Modify enabled alarms</description>
    </command>
    <define>DISHWASHER_ALARM_CLUSTER</define>
    <description>Attributes and commands for configuring the Dishwasher alarm.</description>
    <domain>Appliances</domain>
    <event code="0x00" name="Notify" optional="false" priority="info" side="server">
      <description>Notify</description>
      <field id="0" name="Active" type="AlarmMap"/>
      <field id="1" name="Inactive" type="AlarmMap"/>
      <field id="2" name="State" type="AlarmMap"/>
      <field id="3" name="Mask" type="AlarmMap"/>
    </event>
    <name>Dishwasher Alarm</name>
    <server init="false" tick="false">true</server>
  </cluster>
  <domain name="CHIP"/>
</configurator>
