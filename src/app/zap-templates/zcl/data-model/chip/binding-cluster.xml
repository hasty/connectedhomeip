<?xml version="1.0"?>
<configurator>
  <cluster>
    <attribute code="0x0000" define="BINDING_LIST" entryType="TargetStruct" optional="false" side="server" type="ARRAY" writable="true">
      <access op="write" role="manage"/>
      <description>Binding</description>
    </attribute>
    <code>0x001e</code>
    <define>BINDING_CLUSTER</define>
    <description>The Binding Cluster is meant to replace the support from the Zigbee Device Object (ZDO) for supporting the binding table.</description>
    <domain>General</domain>
    <name>Binding</name>
  </cluster>
  <domain name="CHIP"/>
  <struct isFabricScoped="true" name="TargetStruct">
    <cluster code="0x001e"/>
    <item fieldId="1" name="Node" optional="true" type="node_id"/>
    <item fieldId="2" name="Group" optional="true" type="group_id"/>
    <item fieldId="3" name="Endpoint" optional="true" type="endpoint_no"/>
    <item fieldId="4" name="Cluster" optional="true" type="cluster_id"/>
  </struct>
</configurator>
