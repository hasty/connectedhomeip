    <attribute code="0x05" default="0x00000000" define="BEACON_LOST_COUNT" isNullable="true" max="0xFFFFFFFF" min="0x00000000" optional="true" side="server" type="int32u" writable="false">BeaconLostCount</attribute>
    <attribute code="0x06" default="0x00000000" define="BEACON_RX_COUNT" isNullable="true" max="0xFFFFFFFF" min="0x00000000" optional="true" side="server" type="int32u" writable="false">BeaconRxCount</attribute>
    <attribute code="0x07" default="0x00000000" define="PACKET_MULTICAST_RX_COUNT" isNullable="true" max="0xFFFFFFFF" min="0x00000000" optional="true" side="server" type="int32u" writable="false">PacketMulticastRxCount</attribute>
    <attribute code="0x08" default="0x00000000" define="PACKET_MULTICAST_TX_COUNT" isNullable="true" max="0xFFFFFFFF" min="0x00000000" optional="true" side="server" type="int32u" writable="false">PacketMulticastTxCount</attribute>
    <attribute code="0x09" default="0x00000000" define="PACKET_UNICAST_RX_COUNT" isNullable="true" max="0xFFFFFFFF" min="0x00000000" optional="true" side="server" type="int32u" writable="false">PacketUnicastRxCount</attribute>
    <attribute code="0x0A" default="0x00000000" define="PACKET_UNICAST_TX_COUNT" isNullable="true" max="0xFFFFFFFF" min="0x00000000" optional="true" side="server" type="int32u" writable="false">PacketUnicastTxCount</attribute>
    <attribute code="0x0B" default="0x0000000000000000" define="CURRENT_MAX_RATE" isNullable="true" max="0xFFFFFFFFFFFFFFFF" min="0x0000000000000000" optional="true" side="server" type="int64u" writable="false">CurrentMaxRate</attribute>
    <attribute code="0x0C" default="0x0000000000000000" define="OVERRUN_COUNT" isNullable="true" max="0xFFFFFFFFFFFFFFFF" min="0x0000000000000000" optional="true" side="server" type="int64u" writable="false">OverrunCount</attribute>
    <code>0x0036</code>
    <command cli="chip wifi_network_diagnostics resetcounts" code="0x00" name="ResetCounts" optional="true" source="client">
      <description>Reception of this command SHALL reset the Breacon and Packet related count attributes to 0</description>
    </command>
    <define>WIFI_NETWORK_DIAGNOSTICS_CLUSTER</define>
    <description>The Wi-Fi Network Diagnostics Cluster provides a means to acquire standardized diagnostics metrics that MAY be used by a Node to assist a user or Administrative Node in diagnosing potential problems.</description>
    <domain>General</domain>
    <event code="0x00" name="Disconnection" optional="true" priority="info" side="server">
      <description>Indicate that a Node’s Wi-Fi connection has been disconnected as a result of de-authenticated or dis-association and indicates the reason.</description>
      <field id="0" name="ReasonCode" type="int16u"/>
    </event>
    <event code="0x01" name="AssociationFailure" optional="true" priority="info" side="server">
      <description>Indicate that a Node has failed to connect, or reconnect, to a Wi-Fi access point.</description>
      <field id="0" name="AssociationFailure" type="AssociationFailureCauseEnum"/>
      <field id="1" name="Status" type="int16u"/>
    </event>
    <event code="0x02" name="ConnectionStatus" optional="true" priority="info" side="server">
      <description>Indicate that a Node’s connection status to a Wi-Fi network has changed.</description>
      <field id="0" name="ConnectionStatus" type="ConnectionStatusEnum"/>
    </event>
    <name>WiFi Network Diagnostics</name>
  </cluster>
