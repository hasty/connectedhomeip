<?xml version="1.0"?>
<configurator>
  <bitmap name="Feature" type="bitmap32">
    <cluster code="0x0056"/>
    <field mask="0x01" name="TemperatureNumber"/>
    <field mask="0x02" name="TemperatureLevel"/>
    <field mask="0x04" name="TemperatureStep"/>
  </bitmap>
  <cluster>
    <attribute code="0x0000" define="TEMP_SETPOINT" optional="true" side="server" type="temperature" writable="false">TemperatureSetpoint</attribute>
    <attribute code="0x0001" define="MIN_TEMP" optional="true" side="server" type="temperature" writable="false">MinTemperature</attribute>
    <attribute code="0x0002" define="MAX_TEMP" optional="true" side="server" type="temperature" writable="false">MaxTemperature</attribute>
    <attribute code="0x0003" define="STEP" optional="true" side="server" type="temperature" writable="false">Step</attribute>
    <attribute code="0x0004" define="SELECTED_TEMP_LEVEL" optional="true" side="server" type="int8u" writable="false">SelectedTemperatureLevel</attribute>
    <attribute code="0x0005" define="SUPPORTED_TEMP_LEVELS" entryType="char_string" optional="true" side="server" type="ARRAY" writable="false">SupportedTemperatureLevels</attribute>
    <client init="false" tick="false">true</client>
    <code>0x0056</code>
    <command code="0x00" name="SetTemperature" optional="false" source="client">
      <arg max="MAX_TEMP" min="MIN_TEMP" name="TargetTemperature" optional="true" type="temperature"/>
      <arg name="TargetTemperatureLevel" optional="true" type="int8u"/>
      <description>Set Temperature</description>
    </command>
    <define>TEMPERATURE_CONTROL_CLUSTER</define>
    <description>Attributes and commands for configuring the temperature control, and reporting temperature.</description>
    <domain>Appliances</domain>
    <name>Temperature Control</name>
    <server init="false" tick="false">true</server>
  </cluster>
  <domain name="CHIP"/>
</configurator>
